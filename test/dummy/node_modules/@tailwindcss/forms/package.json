{
  "name": "@tailwindcss/forms",
  "version": "0.3.4",
  "main": "src/index.js",
  "license": "MIT",
  "repository": "https://github.com/tailwindlabs/tailwindcss-forms",
  "publishConfig": {
    "access": "public"
  },
  "prettier": {
    "printWidth": 100,
    "semi": false,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "scripts": {
    "dev": "concurrently \"npm run serve\" \"npm run watch\"",
    "serve": "live-server .",
    "watch": "chokidar \"./src/**/*.js\" -c \"npm run build\"",
    "build": "node scripts/build.js",
    "prepublishOnly": "npm run build"
  },
  "peerDependencies": {
    "tailwindcss": ">=2.0.0"
  },
  "devDependencies": {
    "autoprefixer": "^10.0.2",
    "chokidar-cli": "^2.1.0",
    "clean-css": "^4.2.1",
    "concurrently": "^5.3.0",
    "live-server": "^1.2.1",
    "postcss": "^8.1.7",
    "tailwindcss": "^2.0.3"
  },
  "dependencies": {
    "mini-svg-data-uri": "^1.2.3"
  }
}
